<br>
<p class="lead mt-4"> <a href="../dashboard">Back to dashboard</a></p>
<br>

<% let i= 1 %>
<div class="card-deck">
<% members.forEach((member)=>{ %>

  <% if(i%4==0){ %>
    </div>
    <br>
    <div class="card-deck">
    <% } %>


<div class="card" style="width: 10rem;">
  <img class="card-img-top p-5" src="/<%= member.userImage %>">
  <div class="card-body">

    <h5 class="card-title"><%= member.name %></h5>
    <h6 class="card-subtitle mb-2 text-muted">  <%= member.email %></h6>
    <p class="card-text">Membership ID: <%= member.memberno %> </p>
    <form action="/dashboard/delete/<%= member.id %>" method="post">

        <button type="submit" name="button"><font color="red"><i class="fas fa-trash-alt"></i></font>
        </button>

    </form>
  </div>
</div>

<% i++ %>
<% }) %>
</div>
